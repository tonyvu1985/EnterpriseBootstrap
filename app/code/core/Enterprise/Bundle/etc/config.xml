<config>
    <modules>
        <Enterprise_Bundle>
            <version>1.13.0.0</version>
        </Enterprise_Bundle>
    </modules>
    <global>
        <models>
            <enterprise_bundle>
                <class>Enterprise_Bundle_Model</class>
                <resourceModel>enterprise_bundle_resource</resourceModel>
            </enterprise_bundle>
            <enterprise_bundle_resource>
                <class>Enterprise_Bundle_Model_Resource</class>
            </enterprise_bundle_resource>
        </models>
        <catalog>
            <product>
                <type>
                    <bundle translate="label" module="bundle">
                        <stock_indexer>enterprise_bundle/indexer_stock</stock_indexer>
                    </bundle>
                </type>
            </product>
        </catalog>
    </global>
    <frontend>
        <translate>
            <modules>
                <Enterprise_Bundle>
                    <files>
                        <default>Enterprise_Bundle.csv</default>
                    </files>
                </Enterprise_Bundle>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Enterprise_Bundle>
                    <files>
                        <default>Enterprise_Bundle.csv</default>
                    </files>
                </Enterprise_Bundle>
            </modules>
        </translate>
    </adminhtml>
</config>
